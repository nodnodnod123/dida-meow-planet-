
.container{
  background-color: #ffffff;
  height : 100%;
  background-color: #fffaca;
  padding-bottom: 100%;
}
.partOne {
  height: 10%;    /*高度扩展到页面10%的高度*/
  width: 100%;    /*宽度扩展到全部*/
  display: flex;
  justify-content: space-between;
  background-color: #b6d192;
  padding-top: 80rpx;
  padding-bottom: 130rpx;
}
.userinfo {
  display: flex;
  align-items: center;
  height: 128rpx;
  margin-left: 40rpx;
  color: rgb(255, 255, 255);
}

.userinfo-avatar {   /*用户头像*/
  overflow: hidden;
  width: 150rpx;        /*头像球宽度*/
  height: 150rpx;       /*头像球高度*/
  margin: 60rpx;
  border-radius: 50%;
}

.partTwo{
  height: 500rpx;     /*边框高度*/
  width: 80%;
  margin-left: 10%;       /*视图框与左边界距离*/
  margin-top: -30rpx;    /*视图与上一视图间距*/
  background-color: #bde9e2;    /*视图背景颜色*/
  border: 1rpx solid rgb(150, 150, 150);    /*边框颜色*/
  border-radius: 25rpx;
}

